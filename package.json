{
  "name": "leetcode",
  "version": "1.0.0",
  "description": "leetcode解题进行中",
  "main": "workspace/main.cpp",
  "scripts": {
    "genTOC": "node playground/javascript/genTOC.js",
    "publish": "npm run genTOC && git add . && git commit && git push",
    "clear": "rm -f workspace/*.out",
    "cppRun": "./workspace/main.out",
    "cppCompile": "g++ workspace/main.cpp -o workspace/main.out",
    "cppBoot": "npm run cppCompile && echo '--- cpp output ---\n' && npm run cppRun && echo '\n--- cpp end ---'",
    "cppWatch": "nodemon --delay 3 --watch workspace/main.cpp --exec 'npm run cppBoot'",
    "start": "npm run cppWatch",
    "goRun": "./workspace/main.out",
    "goCompile": "go build -o workspace/main.out workspace/main.go",
    "goBoot": "npm run goCompile && echo '--- golang output ---\n' && npm run goRun && echo '\n--- golang end ---'",
    "goWatch": "nodemon --delay 3 --watch workspace/main.go --exec 'npm run goBoot'",
    "go": "npm run goWatch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/muyids/leetcode.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/muyids/leetcode/issues"
  },
  "homepage": "https://github.com/muyids/leetcode#readme"
}
